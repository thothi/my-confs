separator_block_width=14

[ETH]
label=ETH:
command=curl https://ethereumprice.org/ | grep "class=\"current-price__price\">" | grep -oP '(?<=rp\">).*(?=</span></span>)'
interval=10
color=#00FF00

[memory]
label=Mem:
command=/usr/lib/i3blocks/memory
interval=30

#[swap]
#label=Swap:
#command=/usr/lib/i3blocks/memory swap
#interval=30

[Weather]
command=~/.i3/IconicWeather.sh
interval=120
color=#e97ac8


[disk-root]
label=/:
command=/usr/lib/i3blocks/disk2 / -path / -prune -o
interval=30
color=#1793D1


[disk-home]
label=:
command=/usr/lib/i3blocks/disk $HOME
interval=30
color=#859900

[ssid]
label=
command=echo "$(iw dev wlp5s0 link | grep SSID | cut -d " " -f 2-)"
color=#d33682
interval=5



[wireless]
label=W
instance=wlp5s0
command=/usr/lib/i3blocks/iface
color=#00FF00
interval=10


[ethernet]
label=E
instance=enp4s0
command=/usr/lib/i3blocks/iface
color=#00FF00
interval=10

#[strength]
#command=echo "$(iw dev wlp5s0 link | grep signal | cut -d " " -f 2-)"
#interval=10
#color=#cb4b16

[temp]
label=
command=echo "$(sensors coretemp-isa-0000 | awk '/Physical/ {print $4}')"
interval=10
color=#b58900

[cpu]
label=CPU
command=/usr/lib/i3blocks/cpu_usage
interval=10

[battery]
#label=BAT
label=⚡
command=/usr/lib/i3blocks/battery BAT0
#command=~/.i3/battery BAT0
interval=30

#[backlight]
#label=☼
#command=/usr/lib/i3blocks/xblshort
#interval=1
#color=#ee4d2e

[volume]
label=
command=/usr/lib/i3blocks/volume
interval=1
signal=10
color=#d70a53


[time]
label=
command=date '+%a %d.%m.%y %H:%M:%S'
interval=1
color=#50C878


